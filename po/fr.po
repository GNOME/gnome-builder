#: data/gsettings/org.gnome.builder.project-tree.gschema.xml:10
#: plugins/project-tree/gtk/menus.ui:84
msgid "Show Icons"
msgstr "Montrer les icônes"

#: data/gsettings/org.gnome.builder.project-tree.gschema.xml:11
