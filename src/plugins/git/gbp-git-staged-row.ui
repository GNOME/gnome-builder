<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GbpGitStagedRow" parent="GtkWidget">
    <child>
      <object class="GtkBox">
        <property name="spacing">6</property>
        <child>
          <object class="GtkImage" id="image">
            <binding name="gicon">
              <lookup name="icon" type="GbpGitStagedItem">
                <lookup name="item">GbpGitStagedRow</lookup>
              </lookup>
            </binding>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="title">
            <property name="xalign">0</property>
            <property name="hexpand">true</property>
            <property name="ellipsize">start</property>
            <binding name="tooltip-text">
              <lookup name="title" type="GbpGitStagedItem">
                <lookup name="item">GbpGitStagedRow</lookup>
              </lookup>
            </binding>
            <binding name="label">
              <lookup name="title" type="GbpGitStagedItem">
                <lookup name="item">GbpGitStagedRow</lookup>
              </lookup>
            </binding>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
