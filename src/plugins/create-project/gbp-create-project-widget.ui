                        <property name="action-name">create-project.select-folder</property>
                        <property name="valign">center</property>
                        <property name="icon-name">folder-symbolic</property>
                        <style>
                          <class name="flat"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
