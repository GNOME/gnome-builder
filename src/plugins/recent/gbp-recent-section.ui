<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GbpRecentSection" parent="GtkWidget">
    <child>
      <object class="AdwPreferencesGroup" id="group">
        <property name="title" translatable="yes">Recent Projects</property>
        <child>
          <object class="GtkListBox" id="listbox">
            <property name="selection-mode">none</property>
            <signal name="row-activated" swapped="true" object="GbpRecentSection" handler="gbp_recent_section_row_activated"/>
            <style>
              <class name="boxed-list"/>
            </style>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
